<suite name="Browser Test Suite" parallel ="tests" thread-count="2">
    <test name="Chrome Test">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="test.testcasepositive"/>
        </classes>
    </test>
    <test name="FireFox Test">
    <parameter name="browser" value="firefox"/>
    <classes>
        <class name="test.testcasepositive"/>
    </classes>
</test>
</suite>
